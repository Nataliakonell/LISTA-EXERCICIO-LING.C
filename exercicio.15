
#include <stdio.h>

int main()
{
    int a, b;
    int x = 0;

    system("cls");

    while (x < 5)
    {
        printf("Escreva o par que deseja (par %d): ", x + 1);
        scanf("%d %d", &a, &b);

        if (a < b)
        {
            int y = a;

            while (y <= b)
            {
                printf("%d ", y);
                y++;
            }
            printf("\n");
            x++;
        }
        else
        {
            printf("NÃ£o permitido, 'a' deve ser menor que 'b'.\n");
        }
    }

    return 0;
}
